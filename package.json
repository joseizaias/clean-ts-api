{
  "name": "clean-node-api",
  "version": "2.2.0",
  "description": "",
  "main": "src/main/server.ts",
  "scripts": {
    "start": "node dist/main/server.js",
    "build": "rimraf dist && tsc -p tsconfig-build.json",
    "debug": "nodemon -L --watch ./dist --inspect=0.0.0.0:9222 --nolazy ./dist/main/server.js",
    "debugolds": "node --inspect=0.0.0.0:9222 --nolazy dist/main/server.js",
    "dev": "sucrase-node src/main/server.ts",
    "up": "npm run build && docker-compose up -d",
    "up2": "npm run build && docker-compose up",
    "down": "docker-compose down",
    "jest2": "jest --runInBand --detectOpenHandles",
    "jest": "jest --passWithNoTests  --noStackTrace --runInBand",
    "test": "jest --passWithNoTests  --silent --noStackTrace --runInBand",
    "test:verbose": "jest --passWithNoTests --runInBand",
    "test:unit": "npm test -- --watch -c jest-unit-config.js",
    "test:integration": "npm test -- --watch -c jest-integration-config.js",
    "test:staged": "npm test -- --findRelatedTests",
    "test:ci": "npm test -- --coverage"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "@babel/preset-env": "^7.18.10",
    "@babel/preset-typescript": "^7.18.6",
    "@shelf/jest-mongodb": "^4.0.0",
    "@types/bcrypt": "^5.0.0",
    "@types/express": "^4.17.13",
    "@types/jest": "^28.1.6",
    "@types/jsonwebtoken": "^8.5.9",
    "@types/mongodb": "^3.6.20",
    "@types/node": "^18.6.5",
    "@types/supertest": "^2.0.12",
    "@types/validator": "^13.7.5",
    "@typescript-eslint/eslint-plugin": "^5.33.0",
    "babel-jest": "^28.1.3",
    "babel-plugin-syntax-dynamic-import": "^6.18.0",
    "babel-plugin-transform-runtime": "^6.23.0",
    "eslint": "^8.21.0",
    "eslint-config-standard-with-typescript": "^22.0.0",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-n": "^15.2.4",
    "eslint-plugin-promise": "^6.0.0",
    "git-commit-msg-linter": "^4.1.3",
    "jest": "^28.1.3",
    "lint-staged": "^13.0.3",
    "mockdate": "^3.0.5",
    "supertest": "^6.2.4",
    "ts-jest": "^28.0.8",
    "ts-node": "^10.9.1",
    "typescript": "^4.7.4"
  },
  "dependencies": {
    "bcrypt": "^5.0.1",
    "express": "^4.18.1",
    "jsonwebtoken": "^8.5.1",
    "module-alias": "^2.2.2",
    "mongodb": "3.7",
    "nodemon": "^2.0.19",
    "validator": "^13.7.0"
  },
  "engines": {
    "node": "16.x"
  },
  "_moduleAliases": {
    "@": "dist"
  }
